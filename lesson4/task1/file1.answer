# Take a list of words and returns a list those that start with a specific letter


def starts_with(letter, words):
    answer = []
    for word in words:
        if word.startswith(letter):
            answer.append(word)
    return answer

word_list = ["natasha", "nautical", "heavy", "story", "numbat", "octopus", "nitrogen", "no"]

print starts_with('n', word_list)